<?xml version="1.0" encoding="UTF-8"?>
<Otds xmlns="http://otds-group.org/otds" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="1.9" xsi:schemaLocation="http://otds-group.org/otds ../xsd/otds.xsd">
	<Products>
		<!-- Nur Unterkünfte über "Tag" gefiltert  -->
		<Product Key="AccomOnly" ProductType="AccommodationOnly">
			<Components>
				<Accommodation/>
			</Components>
			<Filter Key="1">
				<Tags Source="Accommodation" Class="PackageOnly"/>
			</Filter>
		</Product>
		<!-- Einfache Kombination aller passenden Flüge mit entsprechenden Unterkünften  -->
		<Product Key="Package1" ProductType="FlightAccommodation">
			<Components>
				<Accommodation/>
				<ReturnFlight/>
			</Components>
			<Filter Key="1">
				<And>
					<MatchEqual>
						<Tag Source="Accommodation" Class="Reiseart"/>
						<Tag Source="ReturnFlight" Class="Reiseart"/>
					</MatchEqual>
					<MatchEqual>
						<Element Source="Accommodation">CatchmentAirport</Element>
						<Element Source="ReturnFlight.Outbound">ArrivalAirport</Element>
					</MatchEqual>
				</And>
			</Filter>
		</Product>
		<!-- Ausschluss einer Unterkunft über Key  -->
		<Product Key="Package2" ProductType="FlightAccommodation">
			<Components>
				<Accommodation/>
				<ReturnFlight/>
			</Components>
			<Filter Key="1">
				<And>
					<Not>
						<Keys Source="Accommodation">FUE102</Keys>
					</Not>
					<MatchEqual>
						<Tag Source="Accommodation" Class="Reiseart"/>
						<Tag Source="ReturnFlight" Class="Reiseart"/>
					</MatchEqual>
					<MatchEqual>
						<Element Source="Accommodation">CatchmentAirport</Element>
						<Element Source="ReturnFlight.Outbound">ArrivalAirport</Element>
					</MatchEqual>
				</And>
			</Filter>
		</Product>
		<!-- Zuordnung der Flüge zu den Unterkünften über Tags  -->
		<Product Key="Package3" ProductType="FlightAccommodation">
			<Components>
				<Accommodation/>
				<ReturnFlight/>
			</Components>
			<Filter Key="1">
				<And>
					<MatchEqual>
						<Tag Source="Accommodation" Class="Reiseart"/>
						<Tag Source="ReturnFlight" Class="Reiseart"/>
					</MatchEqual>
					<MatchEqual>
						<Element Source="Accommodation">CatchmentAirport</Element>
						<Element Source="ReturnFlight.Outbound">ArrivalAirport</Element>
					</MatchEqual>
					<MatchEqual>
						<Tag Source="Accommodation" Class="Combi"/>
						<Tag Source="ReturnFlight" Class="Combi"/>
					</MatchEqual>
				</And>
			</Filter>
		</Product>
		<!-- Eine Produktregel welche den Flug über DefinedComponents enthält  -->
		<Product Key="PackageWithCombiFlights" ProductType="FlightAccommodation">
			<Components>
				<Accommodation/>
				<DefinedComponent UseRole="ReturnFlight"/>
			</Components>
			<Filter Key="1">
				<MatchEqual>
					<Element Source="Accommodation">CatchmentAirport</Element>
					<Element Source="ReturnFlight.Outbound">ArrivalAirport</Element>
				</MatchEqual>
			</Filter>
		</Product>
		<!-- Seit Version 2.0 werden die Produkte, die für die Paketierung mit externen Komponenten bereitgestellt werden, über eine neue Komponente "ExternalComponent" identifiziert und im Gegensatz zu vorher nicht mehr mit dem ProductTyp = "AccommodationOnly" sondern mit  ProductTyp = "FlightAccommodation" geliefert.-->
		<!-- Nachfolgend die Definition von Unterkünften, die mit "externen" Flügen zusammen als Pauschalangebot verkauft werden sollen. -->
		<Product Key="AccomOnlyForMix" ProductType="FlightAccommodation">
			<Components>
				<ExternalComponent Role="Outbound" DayAllocationIndex="-1"/>
				<Accommodation DayAllocationIndex="0"/>
				<ExternalComponent Role="Inbound" DayAllocationIndex="1"/>
			</Components>
			<Filter Key="1">
				<Tags Source="Accommodation" Class="MixOnly"/>
			</Filter>
		</Product>
		<!-- Nachfolgend die Definition von Flügen, die mit "externen" Unterkünften zusammen als Pauschalangebot verkauft werden sollen. -->
		<Product Key="FlightOnlyForMix" ProductType="FlightAccommodation">
			<Components>
				<CombiComponent Role="Outbound">
					<OnewayFlight DayAllocationIndex="-1"/>
				</CombiComponent>
				<ExternalComponent Role="Accommodation" DayAllocationIndex="0"/>
				<CombiComponent Role="Inbound">
					<OnewayFlight DayAllocationIndex="1"/>
				</CombiComponent>
			</Components>
			<Filter Key="1">
				<Tags Source="OnewayFlight" Class="MixOnly"/>
			</Filter>
		</Product>
	</Products>
