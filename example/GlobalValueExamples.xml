	<Products>
		<GlobalValues>
			<!--Hier eine Auswahl von typischen GlobalValues. Suchen Sie jeweils nach dem Key-Value und Sie kommen zu der konkreten Bedingung.   -->
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für den deutschen Markt aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="Market_DE">
				<ParameterSet Key="1">
					<SalesMarket>DE</SalesMarket>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für den Schweizer Markt aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="Market_CH">
				<ParameterSet Key="1">
					<SalesMarket>DE</SalesMarket>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für Schweizer Franken aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<!--Ein entsprechendes praktisches Beispiel hierfür finden Sie im "CurrencyExample.xml"   -->
			<GlobalValue Key="CHF">
				<ParameterSet Key="1">
					<Currency>CHF</Currency>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für Reisebüros (offline) aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="TravelAgencyOnly">
				<ParameterSet Key="1">
					<SalesChannel>TravelAgency</SalesChannel>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung durch TravelTainment aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="TraveltainmentOnly">
				<ParameterSet Key="1">
					<DistributionChannel>TravelTainment</DistributionChannel>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für eine spezifische Agentur aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="Agency001760004Only">
				<ParameterSet Key="1">
					<CrsSystem>Toma</CrsSystem>
					<AgencyCode>001760004</AgencyCode>
					<BrandCode>TOCC</BrandCode>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung für eine Gruppe spezifischer Agenturen aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="AgencyGroupXXXOnly">
				<ParameterSet Key="1">
					<CrsSystem>Toma</CrsSystem>
					<AgencyCode>001760004</AgencyCode>
					<BrandCode>TOCC</BrandCode>
				</ParameterSet>
				<ParameterSet Key="1">
					<CrsSystem>Toma</CrsSystem>
					<AgencyCode>001760005</AgencyCode>
					<BrandCode>TOCC</BrandCode>
				</ParameterSet>
				<ParameterSet Key="1">
					<CrsSystem>Toma</CrsSystem>
					<AgencyCode>001760006</AgencyCode>
					<BrandCode>TOCC</BrandCode>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung aktiv, dass ein Reseller die Daten für die Paketierung nutzt  Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="DataMixReseller">
				<ParameterSet Key="1">
					<Reseller>
						<ResellerType>PackageMixResellers</ResellerType>
					</Reseller>
				</ParameterSet>
			</GlobalValue>
			<!--Dieser GlobalValue wird im Kontext einer Verarbeitung der Daten für spezifische Reseller aktiv. Entsprechende Key- und Tag-Bedingungen auf diesen GlobalValue sind nur in diesem Kontext erfüllt.   -->
			<GlobalValue Key="ResellerAgenciesXXXOnly">
				<ParameterSet Key="1">
					<Reseller>
						<CrsSystem>Toma</CrsSystem>
						<AgencyCode>001760004</AgencyCode>
						<BrandCode>TOCC</BrandCode>
					</Reseller>
				</ParameterSet>
				<ParameterSet Key="2">
					<Reseller>
						<CrsSystem>Toma</CrsSystem>
						<AgencyCode>001760005</AgencyCode>
						<BrandCode>TOCC</BrandCode>
					</Reseller>
				</ParameterSet>
				<ParameterSet Key="3">
					<Reseller>
						<CrsSystem>Toma</CrsSystem>
						<AgencyCode>001760006</AgencyCode>
						<BrandCode>TOCC</BrandCode>
					</Reseller>
				</ParameterSet>
			</GlobalValue>
		</GlobalValues>
		<Product Key="AccomOnly" ProductType="AccommodationOnly">
			<Components>
				<Accommodation/>
			</Components>
			<Filter Key="Standard">
				<Tags Source="Accommodation" Class="PackageOnly"/>
			</Filter>
			<Filter Key="SalesChannel">
				<And>
					<!--Nur im Kontext einer Datenverarbeitung für Reisebüros dürfen die Accommodations kein Tag = "NoTravelAgency" haben  -->
					<Imply>
						<If>
							<Keys Source="GlobalValue">TravelAgencyOnly</Keys>
						</If>
						<Then>
							<Not>
								<Tags Source="Accommodation" Class="SalesChannelExclusion">NoTravelAgency</Tags>
							</Not>
						</Then>
					</Imply>
					<!--Nur im Kontext einer Datenverarbeitung für OTAs dürfen die Accommodations kein Tag = "NoOnlineTravelAgencies" haben  -->
					<Imply>
						<If>
							<Keys Source="GlobalValue">OnlineTravelAgencies</Keys>
						</If>
						<Then>
							<Not>
								<Tags Source="Accommodation" Class="SalesChannelExclusion">NoOnlineTravelAgencies</Tags>
							</Not>
						</Then>
					</Imply>
				</And>
			</Filter>
			<Filter Key="DistributionChannels">
				<!--Nur im Kontext einer Datenverarbeitung für Reisebüros dürfen die Accommodations kein Tag = "NoTravelAgency" haben  -->
				<Imply>
					<If>
						<Keys Source="GlobalValue">TraveltainmentOnly</Keys>
					</If>
					<Then>
						<Duration Source="Product">
							<Max>28</Max>
						</Duration>
					</Then>
				</Imply>
			</Filter>
		</Product>
	</Products>
