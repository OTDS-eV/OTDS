<?xml version="1.0" encoding="UTF-8"?>
<Otds xmlns="http://otds-group.org/otds" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="2.0" xsi:schemaLocation="http://otds-group.org/otds ../xsd/otds.xsd">
	<!-- de: Ein Beispiel fÃ¼r einen Flug von Frankfurt nach Fuerteventura mit der Buchungsklasse Y. 
        Es sind keine Flugzeiten oder weitere Details zu Segmenten angegeben.  -->
       <!-- en: An example of a flight from Frankfurt to Fuerteventura with booking class Y. 
        No flight times or further details on segments are given.  -->
	<Products>
		<Product Key="OneFlightOnly" ProductType="OnewayFlightOnly">
			<Components>
				<OnewayFlight/>
			</Components>
		</Product>
	</Products>
	<Brands/>
	<Flights>
		<OnewayFlights>
			<OnewayFlight Key="FRAFUE">
				<Booking>
					<BookingGroup Area="ServiceArea">
						<BookingParameter Field="ServiceCode">
							<Value>FRA FUE</Value>
						</BookingParameter>
					</BookingGroup>
				</Booking>
				<DepartureAirport>FRA</DepartureAirport>
				<ArrivalAirport>FUE</ArrivalAirport>
				<BookingClass Key="FRAFUE_Y">
					<Booking>
						<BookingGroup Area="ServiceArea">
							<BookingParameter Field="ServiceFeatureCode">
								<Value>Y</Value>
							</BookingParameter>
						</BookingGroup>
					</Booking>
					<Occupancy Key="1">
						<Person>
							<Count>1</Count>
						</Person>
					</Occupancy>
					<Availabilities Key="FRAFUE_Y">
						<Availability Key="2011-11-01" StartDate="2011-11-01" EndDate="2012-04-30">
							<DefaultDayState>
								<Open>38</Open>
								<CheckIn/>
								<CheckOut/>
							</DefaultDayState>
						</Availability>
					</Availabilities>
				</BookingClass>
				<BookingClass Key="FRAFUE_C">
					<Booking>
						<BookingGroup Area="ServiceArea">
							<BookingParameter Field="ServiceFeatureCode">
								<Value>C</Value>
							</BookingParameter>
						</BookingGroup>
					</Booking>
					<Occupancy Key="1">
						<Person>
							<Count>1</Count>
						</Person>
					</Occupancy>
					<Availabilities Key="FRAFUE_C">
						<Availability Key="2011-11-01" StartDate="2011-11-01" EndDate="2012-04-30">
							<DefaultDayState>
								<Request/>
								<CheckIn/>
								<CheckOut/>
							</DefaultDayState>
						</Availability>
					</Availabilities>
					<PriceItems Key="1">
						<PriceItem Class="BusinessClassUpdgrade">
							<Absolute>
								<Value>50</Value>
								<DayBase>x</DayBase>
								<PersonBase>1</PersonBase>
							</Absolute>
						</PriceItem>
					</PriceItems>
				</BookingClass>
				<BookingClass Key="FRAFUE_F">
					<Booking>
						<BookingGroup Area="ServiceArea">
							<BookingParameter Field="ServiceFeatureCode">
								<Value>F</Value>
							</BookingParameter>
						</BookingGroup>
					</Booking>
					<Occupancy Key="1">
						<Person>
							<Count>1</Count>
						</Person>
					</Occupancy>
					<Availabilities Key="FRAFUE_F">
						<Availability Key="2011-11-01" StartDate="2011-11-01" EndDate="2012-04-30">
							<DefaultDayState>
								<Closed/>
								<CheckIn/>
								<CheckOut/>
							</DefaultDayState>
						</Availability>
					</Availabilities>
				</BookingClass>
			</OnewayFlight>
		</OnewayFlights>
	</Flights>
</Otds>