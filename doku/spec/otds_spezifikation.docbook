									<entry><para xml:id="de_5609" xml:lang="de">Code der Marke</para><para xml:id="en_5609" xml:lang="en">BrandCode</para></entry>
									<entry><para xml:id="de_5610" xml:lang="de">GlobalArea</para><para xml:id="en_5610" xml:lang="en">GlobalArea</para></entry>
									<entry><para xml:id="de_5611" xml:lang="de">Brand</para><para xml:id="en_5611" xml:lang="en">Brand</para></entry>
