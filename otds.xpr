<?xml version="1.0" encoding="UTF-8"?>
<project>
    <meta>
        <filters directoryPatterns="" filePatterns="" positiveFilePatterns="" showHiddenFiles="false"/>
        <options>
            <serialized xml:space="preserve">
                <serializableOrderedMap>
                    <entry>
                        <String>key.editor.open.option.pane</String>
                        <Boolean>false</Boolean>
                    </entry>
                    <entry>
                        <String>key.editor.save.option.pane</String>
                        <Boolean>false</Boolean>
                    </entry>
                    <entry>
                        <String>validation.scenario.associations</String>
                        <scenarioAssociation-array>
                            <scenarioAssociation>
                                <field name="url">
                                    <String>xsd/otds-schema-common.xsd</String>
                                </field>
                                <field name="scenarioIds">
                                    <list>
                                        <String>Validate XML Schema with Saxon - Copy</String>
                                    </list>
                                </field>
                                <field name="scenarioTypes">
                                    <list>
                                        <String>Validation_scenario</String>
                                    </list>
                                </field>
                                <field name="scenarioStorageLocations">
                                    <list>
                                        <Byte>2</Byte>
                                    </list>
                                </field>
                            </scenarioAssociation>
                        </scenarioAssociation-array>
                    </entry>
                    <entry>
                        <String>validation.scenarios</String>
                        <validationScenario-array>
                            <validationScenario>
                                <field name="pairs">
                                    <list>
                                        <validationUnit>
                                            <field name="validationType">
                                                <validationUnitType>
                                                    <field name="validationInputType">
                                                        <String>text/xsd</String>
                                                    </field>
                                                </validationUnitType>
                                            </field>
                                            <field name="url">
                                                <String>${currentFileURL}</String>
                                            </field>
                                            <field name="validationEngine">
                                                <validationEngine>
                                                    <field name="engineType">
                                                        <String>Saxon-EE</String>
                                                    </field>
                                                    <field name="allowsAutomaticValidation">
                                                        <Boolean>true</Boolean>
                                                    </field>
                                                </validationEngine>
                                            </field>
                                            <field name="allowAutomaticValidation">
                                                <Boolean>true</Boolean>
                                            </field>
                                            <field name="extensions">
                                                <null/>
                                            </field>
                                            <field name="validationSchema">
                                                <null/>
                                            </field>
                                        </validationUnit>
                                    </list>
                                </field>
                                <field name="type">
                                    <String>Validation_scenario</String>
                                </field>
                                <field name="name">
                                    <String>Validate XML Schema with Saxon - Copy</String>
                                </field>
                            </validationScenario>
                        </validationScenario-array>
                    </entry>
                </serializableOrderedMap>
            </serialized>
        </options>
    </meta>
    <projectTree name="otds.xpr">
        <folder path="doku/"/>
        <folder path="example/"/>
        <folder path="xsd/"/>
    </projectTree>
</project>