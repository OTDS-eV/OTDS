<?xml version="1.0" encoding="UTF-8"?>
<Otds xmlns="http://otds-group.org/otds" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="1.9" xsi:schemaLocation="http://otds-group.org/otds ../xsd/otds.xsd">
	<!-- 
	Ein Beispiel für einen Flug von Frankfurt nach San Francisco über London und Atlanta.
	-->
	<Products>
		<Product Key="OneFlightOnly" ProductType="OnewayFlightOnly">
			<Components>
				<OnewayFlight/>
			</Components>
		</Product>
	</Products>
	<Brands/>
	<Flights>
		<OnewayFlights>
			<OnewayFlight Key="FRASFO">
				<Properties Key="1">
					<PropertyGroup>
						<FlightRoutes>
							<FlightRoute>
								<Departure>
									<Airport>FRA</Airport>
									<LocalTime>19:20</LocalTime>
								</Departure>
								<Arrival>
									<Airport>LON</Airport>
									<LocalTime>20:00</LocalTime>
								</Arrival>
								<FlightType>Feeder</FlightType>
							</FlightRoute>
							<FlightRoute>
								<Departure>
									<Airport>LON</Airport>
									<LocalTime>22:15</LocalTime>
								</Departure>
								<Arrival>
									<Airport>ATL</Airport>
									<DateOffset>1</DateOffset>
									<LocalTime>04:30</LocalTime>
								</Arrival>
								<FlightType>Main</FlightType>
							</FlightRoute>
							<FlightRoute>
								<Departure>
									<Airport>ATL</Airport>
									<DateOffset>1</DateOffset>
									<LocalTime>07:25</LocalTime>
								</Departure>
								<Arrival>
									<Airport>SFO</Airport>
									<DateOffset>1</DateOffset>
									<LocalTime>11:50</LocalTime>
								</Arrival>
								<FlightType>Feeder</FlightType>
							</FlightRoute>
						</FlightRoutes>
					</PropertyGroup>
				</Properties>
				<DepartureAirport>FRA</DepartureAirport>
				<ArrivalAirport>SFO</ArrivalAirport>
				<BookingClass Key="Y">
					<Booking>
						<BookingGroup Area="ServiceArea">
							<BookingParameter Field="ServiceFeatureCode">
								<Value>Y</Value>
							</BookingParameter>
						</BookingGroup>
					</Booking>
					<Occupancy Key="1">
						<Person>
							<MinCount>1</MinCount>
						</Person>
					</Occupancy>
					<Availabilities Key="1">
						<Availability Key="2011-11-01" StartDate="2011-11-01" EndDate="2012-04-30">
							<DefaultDayState>
								<Open/>
								<CheckIn/>
								<CheckOut/>
							</DefaultDayState>
						</Availability>
					</Availabilities>
				</BookingClass>
			</OnewayFlight>
		</OnewayFlights>
	</Flights>
</Otds>