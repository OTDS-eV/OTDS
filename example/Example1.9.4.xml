<?xml version="1.0" encoding="UTF-8"?>
<Otds xmlns="http://otds-group.org/otds" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:functx="http://www.functx.com" xsi:schemaLocation="http://otds-group.org/otds ../xsd/otds.xsd" Version="1.9.4" UpdateMode="New">
	<!-- vereinfachte Products zur Veranschaulichung der Änderungen in 1.9.4 -->
	<Products>
		<Product Key="AccomOnly" ProductType="AccommodationOnly">
			<Components>
				<Accommodation/>
			</Components>
		</Product>
	</Products>
	<Brands>
		<Brand Key="TO1">
			<Booking>
				<BookingGroup Source="ThisComponent" Area="GlobalArea" Class="Default">
					<BookingParameter Field="BrandCode">
						<Value>TO1</Value>
					</BookingParameter>
				</BookingGroup>
			</Booking>
		</Brand>
	</Brands>
	<!--vereinfachte Accommodations zur Veranschaulichung der Änderungen in 1.9.4 -->
	<Accommodations UpdateMode="New">
		<Accommodation Key="BAD020" UpdateMode="New">
			<Properties Key="1">
				<PropertyGroup>
					<!-- in 1.9.4 hinzugefügte Eigenschaften für Zielgruppen u.a.  Backpacker, BusinessTravelers, ClubTourists, CruiseTravelers, GolfTourists, 
                GroupTourists, LongTermTourists, Cyclists, Motorbiker, RoundtripTravelers, SingleWithChild, WheelchairUsers, BeachTravelers, PartyTourists
			 -->
					<AccomodationTargetgroups>Backpacker BusinessTravelers ClubTourists CruiseTravelers GolfTourists GroupTourists LongTermTourists Cyclists Motorbiker RoundtripTravelers SingleWithChild WheelchairUsers BeachTravelers PartyTourists</AccomodationTargetgroups>
					<GeneralIncludedServices>
						<!-- in 1.9.4 hinzugefügte Werte für inkludierte Zusatzleistungen FreeCancellation und FreeRebooking -->
						<GeneralIncludedService>FreeCancellation</GeneralIncludedService>
						<GeneralIncludedService>FreeRebooking</GeneralIncludedService>
					</GeneralIncludedServices>
				</PropertyGroup>
			</Properties>
			<SellingAccom Key="S5-BAD020">
				<Booking>
					<BookingGroup Area="ServiceArea" Source="Accommodation">
						<BookingParameter Field="RequestCode">
							<Value>H</Value>
						</BookingParameter>
						<BookingParameter Field="ServiceCode">
							<Value>BAD020</Value>
						</BookingParameter>
					</BookingGroup>
				</Booking>
				<Board Key="K">
					<Booking>
						<BookingGroup Area="ServiceArea" Source="Accommodation">
							<BookingParameter Name="BoardCode" Field="ServiceFeatureCode" Index="1">
								<Value>K</Value>
							</BookingParameter>
						</BookingGroup>
					</Booking>
					<Properties Key="1">
						<PropertyGroup>
						    <!-- in 1.9.4 hinzugefügte neue Property "Reference" und der Möglichkeit verschiedene Referenzsysteme einzubinden
							 -->
							<BoardInfo>
								<Reference ReferenceSystem="NL-ANVR" ReferenceType="BoardTypeAccommodation">BB</Reference>
							</BoardInfo>
							<BoardType>Breakfast</BoardType>
						</PropertyGroup>
					</Properties>
					<Filter Key="1">
						<Keys Source="global:Unit">DZ EZ Y</Keys>
					</Filter>
				</Board>
				<Unit Key="A3">
					<Tags>
						<Tag Class="Key">A3</Tag>
					</Tags>
					<Properties Key="1">
						<PropertyGroup>
							<!-- in 1.9.4 hinzugefügte neue Property "Reference" und der Möglichkeit verschiedene Referenzsysteme einzubinden. -->
							<UnitInfo>
								<Reference ReferenceSystem="NL-ANVR" ReferenceType="RoomType">3KA</Reference>
							</UnitInfo>
							
							<!-- in 1.9.4 hinzugefügte neue Wert "Budget" in UnitFacilities. -->
							<UnitFacilities>Budget</UnitFacilities>
							
							<!-- in 1.9.4 hinzugefügte neue Property "BookableOnlyFor" mit dem ersten Eintrag "Honeymooner".-->
							<BookableOnlyFor>HoneyMooner</BookableOnlyFor>
							
						</PropertyGroup>
					</Properties>
					<SellingUnit Key="A3-2-0-0-K">
						<Booking>
							<BookingGroup Area="ServiceArea" Source="Accommodation">
								<BookingParameter Field="ServiceFeatureCode" PadLength="3">
									<Value>DZ</Value>
								</BookingParameter>
							</BookingGroup>
						</Booking>
						<Properties Key="1">
							<PropertyGroup>
								<UnitName>Apartment mit 3 Schlafzimmern</UnitName>
								<UnitType>Apartment</UnitType>
							</PropertyGroup>
						</Properties>
						<Occupancy Key="Unit">
							<Person>
								<MinAge>12</MinAge>
								<Count>5</Count>
							</Person>
						</Occupancy>
					</SellingUnit>
				</Unit>
				<PriceItems Key="HotelOnlyPrices">
					<PriceItem Class="Baseprice">
						<Absolute>
							<Value>79</Value>
							<DayBase>x</DayBase>
							<PersonBase>1</PersonBase>
						</Absolute>
						<Properties>
							<PropertyGroup>
								<!-- in 1.9.4 hinzugefügte neue PriceTypes LastMinute, FirstMinute, UltraLastMinute, UltraFirstMinute.-->
								<PriceType>LastMinute</PriceType>
							</PropertyGroup>
						</Properties>
					</PriceItem>
				</PriceItems>
			</SellingAccom>
			<CatchmentAirports>BAD</CatchmentAirports>
		</Accommodation>
	</Accommodations>
</Otds>
